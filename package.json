{
  "name": "nubian-wizard-sdk",
  "version": "1.0.2",
  "description": "sdk for connecting Nubian Wizard Contracts. ",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blockchain-Battleship/battleship-sdk.git"
  },
  "keywords": [
    "blockchain",
    "battleship",
    "sdk",
    "connect",
    "contracts",
    "ethereum",
    "wallet",
    "blockchain-battleship",
    "polygon",
    "biconomy",
    "cocos"
  ],
  "author": "Ebube Ud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Blockchain-Battleship/battleship-sdk/issues"
  },
  "homepage": "https://github.com/Blockchain-Battleship/battleship-sdk#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "web3": "^1.6.0"
  }
}
