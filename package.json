{
  "name": "nubian-coven-sdk",
  "version": "1.0.17",
  "description": "sdk for connecting Nubian Coven Contracts. ",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test:node": "npx hardhat node --fork  $npm_config_moralis_url &",
    "test:jest": "sleep 20 && jest --config jestconfig.json --runInBand",
    "test": "npm run test:node && npm run test:jest",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NestcoinCo/nubian-coven-sdk"
  },
  "keywords": [
    "nubian",
    "coven",
    "sdk",
    "connect",
    "contracts",
    "ethereum",
    "wallet",
    "aggregator",
    "bsc",
    "nestcoin"
  ],
  "author": "Ebube Ud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NestcoinCo/nubian-coven-sdk/issues"
  },
  "homepage": "https://github.com/NestcoinCo/nubian-coven-sdk#readme",
  "devDependencies": {
    "@jest/types": "^27.4.2",
    "@nomiclabs/hardhat-ethers": "^2.0.4",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@types/chai": "^4.3.0",
    "@types/jest": "^27.0.2",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.7",
    "bignumber.js": "^9.0.2",
    "chai": "^4.3.6",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.4",
    "hardhat": "^2.8.3",
    "jest": "^27.3.1",
    "mocha": "^9.2.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "source-map-support": "^0.5.21",
    "web3": "^1.6.1"
  }
}
